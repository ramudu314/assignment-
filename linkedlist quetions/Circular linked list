/*
class Node {
    Node next;
    int val;

    Node(int val) {
        this.val = val;
        next = null;
    }
}
*/

public static int check(Node head) {
    //enter your code here
    Node curr = head;
    int ans =0;
    while(curr != null){
        if(curr.next == head){
            ans = 1;
            break;
        }
        curr = curr.next;
    }
    return ans;
}
